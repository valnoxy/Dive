    <system:String x:Key="CaptureSettingsCaptureDirTitle">Capture directory</system:String>
    <system:String x:Key="CaptureSettingsCaptureDirDescription">The source directory of the path to be captured.</system:String>

    <!-- Capture Save To Page -->
    <system:String x:Key="CaptureSaveToTitle">Where should the image be saved?</system:String>
    <system:String x:Key="CaptureSaveToDescription">Please select the destination to continue.</system:String>
    <system:String x:Key="CaptureSaveToFileName">File name</system:String>
    <system:String x:Key="CaptureSaveToFileNameDescription">The file name of the image.</system:String>
    <system:String x:Key="CaptureSaveToDisksCounter">Dive Disks loaded: {0}</system:String>

    <!-- Capturing Page -->
    <system:String x:Key="CapturingTitle">Capturing Windows</system:String>
    <system:String x:Key="CapturingDescription">Dive will now capture the specified source. This might take a while.</system:String>
    <system:String x:Key="CapturingWaiting">Waiting ...</system:String>

</ResourceDictionary>