    <system:String x:Key="InjectDriversDescription">Load and inject multiple drivers (Vista and up).</system:String>
    <system:String x:Key="NoSourceSelected">No source directory selected.</system:String>

    <system:String x:Key="MoreSettingsTitle">More Settings</system:String>
    <system:String x:Key="MoreSettingsDescription">See a list of other settings for deployment.</system:String>
    <system:String x:Key="MoreSettingsBtn">Configure</system:String>

    <system:String x:Key="SettingsImportExportTitle">Settings Import &amp; Export</system:String>
    <system:String x:Key="SettingsImportExportDescription">Save and load the current settings.</system:String>
    <system:String x:Key="ImportBtn">Import</system:String>
    <system:String x:Key="ExportBtn">Export</system:String>
    
