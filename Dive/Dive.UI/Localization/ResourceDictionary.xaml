    <system:String x:Key="InstallPageTitle">Installing Windows</system:String>
    <system:String x:Key="InstallPageDescription">Your computer will restart several times. This might take a while.</system:String>
    <system:String x:Key="InstallPageWaiting">Waiting ...</system:String>

