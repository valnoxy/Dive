    <system:String x:Key="CaptureSettingsTitle">How do you want to configure the capture?</system:String>
    <system:String x:Key="CaptureSettingsDescription">Please configure the capture process to continue.</system:String>
    <system:String x:Key="CaptureSettingsImageCardTitle">Image Information</system:String>
    <system:String x:Key="CaptureSettingsImageCardDescription">Details about your Image.</system:String>
