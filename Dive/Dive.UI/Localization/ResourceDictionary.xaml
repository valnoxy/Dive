    <system:String x:Key="Deployment">Deployment</system:String>
    <system:String x:Key="OOBE">Out-Of-Box-Experience</system:String>
    <system:String x:Key="Application">Application</system:String>

    <!-- Disk Selection Page -->
    <system:String x:Key="DiskSelectTitle">Where do you want to install Windows?</system:String>
    <system:String x:Key="DiskSelectDescription">Please select from one of the available disks to continue.</system:String>
    <system:String x:Key="AddRecovery">Add Recovery</system:String>
    <system:String x:Key="UseNTLDR">Use NTLDR (XP)</system:String>
    
