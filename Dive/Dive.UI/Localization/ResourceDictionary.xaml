    <system:String x:Key="Cloud">Cloud</system:String>
    <system:String x:Key="Tweaks">Tweaks</system:String>
    <system:String x:Key="Console">Console</system:String>
    <system:String x:Key="About">About</system:String>
    <system:String x:Key="FunSettingsToolTip">Fun Settings</system:String>

    <system:String x:Key="Cancel">Cancel</system:String>
    <system:String x:Key="Back">Back</system:String>
    <system:String x:Key="Next">Next</system:String>

    <!-- Apply Page (SKU) -->
    <system:String x:Key="ApplySKUTitle">Select the operating system you want to install</system:String>
    <system:String x:Key="ApplySKUTitleDescription">Please select from one of the available options to continue.</system:String>
    <system:String x:Key="ReloadBtn">Reload</system:String>
    <system:String x:Key="ImagesCounter">Images loaded: {0}</system:String>

    <!-- Apply Page (Configure) -->
    <system:String x:Key="ConfigureTitle">How do you want to configure Windows?</system:String>
    <system:String x:Key="ConfigureTitleDescription">Please configure Windows to continue.</system:String>
    
    <system:String x:Key="AdminAccountTitle">Administrator account</system:String>
    <system:String x:Key="AdminAccountDescription">Settings for the new Administrator account.</system:String>
    <system:String x:Key="Username">Username</system:String>
    <system:String x:Key="Password">Password</system:String>
    <system:String x:Key="UsernamePlaceholder">Local Username</system:String>
    <system:String x:Key="PasswordPlaceholder">Password of Local User</system:String>

